package fielden.com.ua.search;

public class Edge {
	public Vertex sourseVertex;
	public Vertex destinationVertex;
	Edge(Vertex sourseVertex, Vertex destinationVertex){
		this.sourseVertex = sourseVertex;
		this.destinationVertex = destinationVertex;
	}
	public Vertex getSourseVertex() {
		return sourseVertex;
	}
	public Edge setSourseVertex(Vertex sourseVertex) {
		this.sourseVertex = sourseVertex;
		return this;
	}
	public Vertex getDestinationVertex() {
		return destinationVertex;
	}
	public Edge setDestinationVertex(Vertex destinationVertex) {
		this.destinationVertex = destinationVertex;
		return this;
	}

}
